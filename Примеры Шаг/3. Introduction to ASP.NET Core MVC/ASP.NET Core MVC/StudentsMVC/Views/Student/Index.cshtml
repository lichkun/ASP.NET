<div style="padding-top:30px">
    <h3>Список студентов</h3>
    <table class="table">
        <tr><td><p>Имя</p></td><td><p>Фамилия</p></td><td><p>Возраст</p></td><td><p>Средний балл</p></td></tr>
        @foreach (var st in ViewBag.Students)
        {
            <tr><td><p>@st.Name</p></td><td><p>@st.Surname</p></td><td><p>@st.Age</p></td><td><p>@st.GPA</p></td></tr>
        }
    </table>
</div>
